{
  "name": "smart-tourist-safety-system",
  "version": "1.0.0",
  "description": "AI-powered tourist safety monitoring system with blockchain digital ID, geo-fencing, and incident response for Indian tourism",
  "main": "src/server.ts",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "eslint src --ext .ts,.js --fix",
    "format": "prettier --write src/**/*.{ts,js,json}",
    "docker:build": "docker build -t tourist-safety-system .",
    "docker:dev": "docker-compose up -d",
    "migrate": "knex migrate:latest",
    "seed": "knex seed:run",
    "blockchain:deploy": "hardhat deploy --network localhost",
    "blockchain:test": "hardhat test"
  },
  "keywords": [
    "tourist-safety",
    "blockchain",
    "ai",
    "geofencing",
    "incident-response",
    "digital-id",
    "india-tourism",
    "northeast-india"
  ],
  "author": "Tourist Safety Development Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@nestjs/common": "^10.2.7",
    "@nestjs/core": "^10.2.7",
    "@nestjs/platform-express": "^10.2.7",
    "@nestjs/config": "^3.1.1",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/swagger": "^7.1.14",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "^10.2.7",
    "@nestjs/platform-socket.io": "^10.2.7",
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "typeorm": "^0.3.17",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "mongoose": "^7.6.3",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "class-validator": "^0.14.0",
    "class-transformer": "^0.5.1",
    "helmet": "^7.0.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "node-cron": "^3.0.2",
    "twilio": "^4.19.0",
    "nodemailer": "^6.9.7",
    "firebase-admin": "^11.11.0",
    "@tensorflow/tfjs-node": "^4.13.0",
    "ml-matrix": "^6.10.7",
    "geolib": "^3.3.4",
    "turf": "^3.0.14",
    "@turf/turf": "^6.5.0",
    "web3": "^4.2.1",
    "ethers": "^6.8.1",
    "hardhat": "^2.17.2",
    "@openzeppelin/contracts": "^4.9.3",
    "ipfs-http-client": "^60.0.1",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "qrcode": "^1.5.3",
    "uuid": "^9.0.1",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "axios": "^1.6.0",
    "google-translate": "^3.0.0",
    "@google-cloud/translate": "^8.0.2",
    "i18n": "^0.15.1",
    "crypto-js": "^4.2.0",
    "otp-generator": "^4.0.1",
    "rate-limiter-flexible": "^3.0.5",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "@types/node": "^20.8.6",
    "@types/express": "^4.17.20",
    "@types/bcryptjs": "^2.4.5",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/passport": "^1.0.14",
    "@types/passport-jwt": "^3.0.11",
    "@types/passport-local": "^1.0.37",
    "@types/cors": "^2.8.15",
    "@types/compression": "^1.7.4",
    "@types/morgan": "^1.9.7",
    "@types/multer": "^1.4.9",
    "@types/uuid": "^9.0.6",
    "@types/qrcode": "^1.5.4",
    "@types/crypto-js": "^4.2.1",
    "@types/jest": "^29.5.6",
    "@types/supertest": "^2.0.15",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "eslint": "^8.52.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "prettier": "^3.0.3",
    "@nomicfoundation/hardhat-ethers": "^3.0.4",
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "chai": "^4.3.10",
    "docker": "^1.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.{ts,js}",
      "!src/**/*.d.ts",
      "!src/**/*.spec.ts",
      "!src/**/*.test.ts"
    ],
    "testMatch": [
      "**/__tests__/**/*.{ts,js}",
      "**/*.(test|spec).{ts,js}"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/smart-tourist-safety-system.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/smart-tourist-safety-system/issues"
  },
  "homepage": "https://github.com/your-org/smart-tourist-safety-system#readme"
}
